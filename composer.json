{
    "name": "firman/travel-api",
    "description": "Travel APIs fetch data from different providers, such as airlines, hotels, destinations, etc.",
    "keywords": ["travel", "api", "flight", "hotel"],
    "type": "library",
    "homepage": "https://github.com/jscoder-io/travel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Firman",
            "email": "jscoder.io@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "symfony/cache": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Firman\\TravelApi\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "phpstan/phpstan": "^1.8",
        "symfony/dotenv": "^6.1",
        "friendsofphp/php-cs-fixer": "^3.11"
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "phpstan": "./vendor/bin/phpstan analyse",
        "cs-fixer": "./vendor/bin/php-cs-fixer fix src"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
